    "import numpy as np\n",
    "class Solution:\n",
    "    def heightChecker(self, heights: List[int]) -> int:\n",
    "        tmp1 = np.array(heights)\n",
    "        tmp2 = np.array(sorted(heights))\n",
    "        return len(tmp1) - list(tmp1 - tmp2).count(0)\n"
   ]
