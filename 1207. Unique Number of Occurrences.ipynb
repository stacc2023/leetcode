   "source": [
    "class Solution:\n",
    "    def uniqueOccurrences(self, arr: List[int]) -> bool:\n",
    "        lst = []\n",
    "        for i in list(set(arr)):\n",
    "            cnt = arr.count(i)\n",
    "            if cnt in lst:\n",
    "                return False\n",
    "            lst.append(cnt)\n",
    "        return True"
   ]
  }
