   "source": [
    "class Solution:\n",
    "    def subtractProductAndSum(self, n: int) -> int:\n",
    "        tmp = [*str(n)]\n",
    "        sum_digits = sum([int(n) for n in tmp])\n",
    "        product_digits = 1\n",
    "        for n in tmp:\n",
    "            product_digits *= int(n)\n",
    "        return product_digits - sum_digits"
   ]
  }
