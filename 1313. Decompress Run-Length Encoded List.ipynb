    "class Solution:\n",
    "    def decompressRLElist(self, nums: List[int]) -> List[int]:\n",
    "        ans = []\n",
    "        for i in range(0,len(nums),2):\n",
    "            ans += [nums[i+1]] * nums[i]\n",
    "        return ans"
   ]
