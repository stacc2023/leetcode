   "source": [
    "class Solution:\n",
    "    def maximum69Number (self, num: int) -> int:\n",
    "        index_6 = str(num).find('6')\n",
    "        if index_6 == -1:\n",
    "            return num\n",
    "        return num + 3*10**(len(str(num)) - index_6 - 1)\n"
   ]
  }
