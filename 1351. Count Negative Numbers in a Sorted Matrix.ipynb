    "class Solution:\n",
    "    def countNegatives(self, grid: List[List[int]]) -> int:\n",
    "        count = 0\n",
    "        for row in grid:\n",
    "            for value in row:\n",
    "                if value < 0:\n",
    "                    count += 1\n",
    "        return count"
   ]
