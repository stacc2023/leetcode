    "class Solution:\n",
    "    def minSubsequence(self, nums: List[int]) -> List[int]:\n",
    "        nums.sort(reverse=True)\n",
    "        for i in range(len(nums)):\n",
    "            if sum(nums[:i+1]) > sum(nums[i+1:]):\n",
    "                return nums[:i+1]"
   ]
