   "source": [
    "class Solution:\n",
    "    def restoreString(self, s: str, indices: List[int]) -> str:\n",
    "        ans = [0] * len(s)\n",
    "        for i in range(len(s)):\n",
    "            ans[indices[i]] = s[i]\n",
    "        return ''.join(ans)"
   ]
