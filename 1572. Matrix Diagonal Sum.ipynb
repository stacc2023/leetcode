    "class Solution:\n",
    "    def diagonalSum(self, mat: List[List[int]]) -> int:\n",
    "        ans = sum(mat[i][i] + mat[i][len(mat) - 1 - i]for i in range(len(mat)))\n",
    "        if len(mat) % 2:\n",
    "            ans -= mat[len(mat) // 2][len(mat) // 2]\n",
    "        return ans\n",
    "            "
