    "class Solution:\n",
    "    def sumOddLengthSubarrays(self, arr: List[int]) -> int:\n",
    "        ans = 0\n",
    "        for i in range(1, len(arr)+1, 2):\n",
    "            for j in range(len(arr) - i + 1):\n",
    "                ans += sum(arr[j:j+i])\n",
    "        return ans"
   ]
