   "source": [
    "class Solution:\n",
    "    def maxDepth(self, s: str) -> int:\n",
    "        stack = []\n",
    "        ans = 0\n",
    "        for c in s:\n",
    "            if c == '(':\n",
    "                stack.append(c)\n",
    "                ans = max(ans, len(stack))\n",
    "            if c == ')':\n",
    "                stack.pop()\n",
    "        return ans"
   ]
  }
