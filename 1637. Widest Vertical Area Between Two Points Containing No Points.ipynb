    "class Solution:\n",
    "    def maxWidthOfVerticalArea(self, points: List[List[int]]) -> int:\n",
    "        tmp = sorted([x for x,y in points])\n",
    "        return max(abs(tmp[i-1] - tmp[i]) for i in range(1, len(tmp)))\n",
    "            \n",
    "        "
