   "source": [
    "from functools import reduce\n",
    "\n",
    "class Solution:\n",
    "    def arrayStringsAreEqual(self, word1: List[str], word2: List[str]) -> bool:\n",
    "        word1 = reduce(lambda s, v: s + v, word1)\n",
    "        word2 = reduce(lambda s, v: s + v, word2)\n",
    "        return word1 == word2"
   ]
  }
