    "class Solution:\n",
    "    def mergeAlternately(self, word1: str, word2: str) -> str:\n",
    "        ans = []\n",
    "        for i in range(min(len(word1), len(word2))):\n",
    "            ans.append(word1[i])\n",
    "            ans.append(word2[i])\n",
    "        return ''.join(ans) + word1[i+1:] + word2[i+1:]"
   ]
  }
