    "class Solution:\n",
    "    def minOperations(self, nums: List[int]) -> int:\n",
    "        count = 0\n",
    "        for i in range(1, len(nums)):\n",
    "            diff = nums[i-1] - nums[i]\n",
    "            if diff >= 0:\n",
    "                nums[i] += diff + 1\n",
    "                count += diff + 1\n",
    "        return count"
   ]
  }
