    "class Solution:\n",
    "    def replaceDigits(self, s: str) -> str:\n",
    "        s = [*s]\n",
    "        for i in range(len(s)):\n",
    "            if i % 2:\n",
    "                s[i] = chr(ord(s[i-1]) + int(s[i]))\n",
    "        return ''.join(s)"
   ]
  }
