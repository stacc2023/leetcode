   "source": [
    "class Solution:\n",
    "    def areOccurrencesEqual(self, s: str) -> bool:\n",
    "        cnt = s.count(s[0])\n",
    "        for n in list(set(s)):\n",
    "            if s.count(n) != cnt:\n",
    "                return False\n",
    "        return True\n"
   ]
  }
