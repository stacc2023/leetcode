    "class Solution:\n",
    "    def reversePrefix(self, word: str, ch: str) -> str:\n",
    "        index = word.find(ch)\n",
    "        if index != -1:\n",
    "            return word[:index+1][::-1] + word[index+1:]\n",
    "        return word"
   ]
