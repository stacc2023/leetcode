    "class Solution:\n",
    "    def countOperations(self, num1: int, num2: int) -> int:\n",
    "        count = 0\n",
    "        while num1 and num2:\n",
    "            if num1 < num2:\n",
    "                num2 -= num1\n",
    "            else:\n",
    "                num1 -= num2\n",
    "            count += 1\n",
    "        return count"
   ]
