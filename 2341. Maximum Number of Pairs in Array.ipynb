   "source": [
    "class Solution:\n",
    "    def numberOfPairs(self, nums: List[int]) -> List[int]:\n",
    "        cnt = [0,0]\n",
    "        for i in list(set(nums)):\n",
    "            cnt[0] += nums.count(i) // 2\n",
    "            cnt[1] += nums.count(i) % 2\n",
    "        return cnt"
   ]
  }
