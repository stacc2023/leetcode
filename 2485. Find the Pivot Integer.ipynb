    "class Solution:\n",
    "    def pivotInteger(self, n: int) -> int:\n",
    "        for i in range(1, n+1):\n",
    "            if i*(i+1)/2 == n*(n+1)/2-i*(i-1)/2:\n",
    "                return i\n",
    "        return -1"
   ]
