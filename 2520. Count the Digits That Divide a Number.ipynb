   "source": [
    "class Solution:\n",
    "    def countDigits(self, num: int) -> int:\n",
    "        ans = 0\n",
    "        tmp = num\n",
    "        while tmp:\n",
    "            if num % (tmp % 10) == 0:\n",
    "                ans += 1\n",
    "            tmp = tmp // 10\n",
    "        return ans"
   ]
  }
