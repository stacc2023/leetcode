    "class Solution:\n",
    "    def separateDigits(self, nums: List[int]) -> List[int]:\n",
    "        ans = []\n",
    "        for n in nums:\n",
    "            for c in str(n):\n",
    "                ans.append(int(c))\n",
    "        return ans"
   ]
