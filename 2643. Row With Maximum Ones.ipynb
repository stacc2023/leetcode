    "class Solution:\n",
    "    def rowAndMaximumOnes(self, mat: List[List[int]]) -> List[int]:\n",
    "        max_count = [0, 0]\n",
    "        for i, row in enumerate(mat):\n",
    "            if max_count[1] < row.count(1):\n",
    "                max_count = [i, row.count(1)]\n",
    "        return max_count"
   ]
