    "class Solution:\n",
    "    def finalString(self, s: str) -> str:\n",
    "        ans = ''\n",
    "        for c in s:\n",
    "            if c == 'i':\n",
    "                ans = ans[::-1]\n",
    "            else:\n",
    "                ans += c\n",
    "        return ans"
   ]
  }
