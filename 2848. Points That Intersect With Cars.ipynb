   "source": [
    "class Solution:\n",
    "    def numberOfPoints(self, nums: List[List[int]]) -> int:\n",
    "        path = set()\n",
    "        for n in nums:\n",
    "            path = path | set(range(n[0], n[1] + 1))\n",
    "        return len(path)"
   ]
