   "source": [
    "class Solution:\n",
    "    def sumIndicesWithKSetBits(self, nums: List[int], k: int) -> int:\n",
    "        tmp = [bin(n).count('1') == k for n in range(len(nums))]\n",
    "        return sum(nums[int(index)] if boolean else 0 for index, boolean in enumerate(tmp))"
   ]
  }
