    "class Solution:\n",
    "    def findWordsContaining(self, words: List[str], x: str) -> List[int]:\n",
    "        ans = []\n",
    "        for index, word in enumerate(words):\n",
    "            if x in word:\n",
    "                ans.append(index)\n",
    "        return ans"
   ]
