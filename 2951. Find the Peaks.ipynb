    "class Solution:\n",
    "    def findPeaks(self, mountain: List[int]) -> List[int]:\n",
    "        ans = []\n",
    "        for i in range(1, len(mountain)-1):\n",
    "            if mountain[i-1] < mountain[i] and mountain[i+1] < mountain[i]:\n",
    "                ans.append(i)\n",
    "        return ans"
   ]
  }
