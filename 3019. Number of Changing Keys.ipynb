   "source": [
    "class Solution:\n",
    "    def countKeyChanges(self, s: str) -> int:\n",
    "        cnt = 0\n",
    "        for i in range(1,len(s)):\n",
    "            if s[i].lower() != s[i-1].lower():\n",
    "                cnt += 1\n",
    "        return cnt"
   ]
  }
