   "source": [
    "class Solution:\n",
    "    def scoreOfString(self, s: str) -> int:\n",
    "        ans = 0\n",
    "        for i in range(len(s) - 1):\n",
    "            ans += abs(ord(s[i]) - ord(s[i+1]))\n",
    "        return ans"
   ]
