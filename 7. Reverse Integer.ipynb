    "class Solution:\n",
    "    def reverse(self, x: int) -> int:\n",
    "        x = int(str(x)[::-1]) if x >= 0 else -int(str(x)[:0:-1])\n",
    "\n",
    "        if x > 2**31 - 1 or x < -2**31:\n",
    "            x = 0\n",
    "\n",
    "        return x"
   ]
