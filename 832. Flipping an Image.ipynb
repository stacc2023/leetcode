    "import numpy as np\n",
    "class Solution:\n",
    "    def flipAndInvertImage(self, image: List[List[int]]) -> List[List[int]]:\n",
    "        arr = np.array(image)\n",
    "        return list((arr[:,::-1] - 1) * -1)"
