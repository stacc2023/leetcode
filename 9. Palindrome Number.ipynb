    "class Solution:\n",
    "    def isPalindrome(self, x: int) -> bool:\n",
    "        s = str(x)\n",
    "        for i, c in enumerate(s):\n",
    "            if s[len(s) - i - 1] != c:\n",
    "                return False\n",
    "        return True"
   ]
