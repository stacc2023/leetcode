    "class Solution:\n",
    "    def sortArrayByParity(self, nums: List[int]) -> List[int]:\n",
    "        even = []\n",
    "        odd = []\n",
    "        for n in nums:\n",
    "            if n % 2:\n",
    "                odd.append(n)\n",
    "            else:\n",
    "                even.append(n)\n",
    "        return even + odd"
   ]
